    "date-fns": "2.29.2",
    "dotenv": "16.0.2",
    "gray-matter": "4.0.3",
    "highlight.js": "11.6.0",
    "next": "12.2.5",
    "next-themes": "0.2.0",
    "node-fetch": "^3.2.10",
    "postcss": "8.4.16",
    "react": "18.2.0",
    "react-dom": "18.2.0",
