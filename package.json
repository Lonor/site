  "dependencies": {
    "@sagi.io/workers-kv": "0.0.13",
    "dotenv": "16.0.3",
    "fanfou-sdk": "5.0.0",
    "gray-matter": "4.0.3",
    "highlight.js": "11.6.0",
    "next": "13.0.1",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "rehype-autolink-headings": "6.1.1",
    "rehype-external-links": "2.0.1",
    "rehype-highlight": "6.0.0",
    "rehype-slug": "5.1.0",
    "rehype-stringify": "9.0.3",
    "rehype-video": "1.2.2",
